GET / VinnerControler.index()
GET /news/{id} VinnerControler.index({id})
GET /submission/{id}/{title} VinnerControler.submission({id}, {title})
GET /submit VinnerControler.submitForm()
POST /submit VinnerControler.submit({req.param.title}, {req.param.link})
GET /login VinnerControler.loginForm()
POST /login VinnerControler.login({req.param.login}, {req.param.password}, {req.param.rememberMe})
GET /logout VinnerControler.logout()
